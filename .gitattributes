test/longtest/ export-ignore

# Exclude other test directories that might have large files
**/testdata/ export-ignore

# Exclude documentation images (they're large and not needed for module usage)
docs/topics/services/img/ export-ignore

# Exclude build artifacts and deployment files
deploy/ export-ignore
