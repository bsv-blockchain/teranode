{
  "name": "dashboard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --open",
    "build": "rm -rf build/_app/immutable && vite build && touch build/.gitkeep",
    "build:default": "vite build",
    "preview": "vite preview",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "perf:dev": "cd perf/dev && lighthouse http://localhost:5173/ --output html --output json --view && cd ../..",
    "perf": "cd perf/preview && lighthouse http://localhost:4173/ --output html --output json --view && cd ../.."
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.10",
    "@neoconfetti/svelte": "^1.0.0",
    "@playwright/test": "^1.28.1",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/blocks": "^7.6.10",
    "@storybook/svelte": "^7.6.10",
    "@storybook/sveltekit": "^7.6.10",
    "@storybook/testing-library": "^0.2.2",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.30.3",
    "@types/cookie": "^0.5.1",
    "@types/d3": "^7.4.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "echarts": "^5.4.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-svelte": "^2.30.0",
    "lighthouse": "^11.4.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.10",
    "svelte": "^4.2.9",
    "svelte-check": "^3.6.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.5.2",
    "vitest": "^0.32.2"
  },
  "type": "module",
  "dependencies": {
    "@sentry/sveltekit": "^7.94.1",
    "@zerodevx/svelte-toast": "^0.9.5",
    "d3": "^7.8.5",
    "i18next": "^23.7.6",
    "polished": "^4.2.2",
    "svelte-i18next": "^2.2.2",
    "tippy.js": "^6.3.7"
  }
}
